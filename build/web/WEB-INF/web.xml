<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    <servlet>
    <servlet-name>InsertComplain</servlet-name>
    <servlet-class>com.leapfrog.Servlet.InsertComplain</servlet-class>
    </servlet>
    
    <servlet>
    <servlet-name>LoadServlet</servlet-name>
    <servlet-class>com.leapfrog.Servlet.LoadServlet</servlet-class>
    </servlet>
    
     <servlet>
    <servlet-name>ReadWhole</servlet-name>
    <servlet-class>com.leapfrog.Servlet.ReadWholeServlet</servlet-class>
    </servlet>

    
    <servlet-mapping>
    <servlet-name>InsertComplain</servlet-name>
    <url-pattern>/insertcomplain</url-pattern>    
     </servlet-mapping> 
     

    
    <servlet-mapping>
    <servlet-name>LoadServlet</servlet-name>
    <url-pattern>/LoadServlet</url-pattern>    
    </servlet-mapping>
    
    <servlet-mapping>
    <servlet-name>ReadWhole</servlet-name>
    <url-pattern>/ReadWhole</url-pattern>    
     </servlet-mapping> 
        
   
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
